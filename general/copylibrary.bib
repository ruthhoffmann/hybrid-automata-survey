journal = {Expert Systems with Applications},
keywords = {Defeasible Logic Programming,Defeasible argumentation,Hybrid Logic Model Checker,Hybrid Logics,Model checking,Safety systems},
pages = {367--385},
title = {{Assuring safety in air traffic control systems with argumentation and model checking}},
volume = {44},
year = {2016}
}
