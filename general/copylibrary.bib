journal = {Robotics and Autonomous Systems},
keywords = {Autonomous systems,BDI programs,Ethics,Formal verification},
pages = {1--14},
publisher = {Elsevier B.V.},
title = {{Formal verification of ethical choices in autonomous systems}},
